  rdfs:domain [ owl:unionOf (okh:Module okh:Software) ] ;
  .

okh:installationGuide
  a owl:DatatypeProperty ;
  rdfs:label "Installation Guide" ;
#  rdfs:subClassOf schema:URL ;
  rdfs:domain okh:Software ;
  .

