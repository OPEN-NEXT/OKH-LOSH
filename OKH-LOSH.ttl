  .

okh:hasManufacturingInstructions
  a owl:ObjectProperty ;
  rdfs:label "has Manufacturing Instructions" ;
  rdfs:range okh:ManufacturingInstructions ;
  rdfs:domain [ owl:unionOf (okh:Module okh:Part) ] ;
  .

okh:relatedTsDC
  a owl:ObjectProperty ;
  rdfs:label "related TsDC" ;
