  schema:supersededBy :OTRL4 ;
  owl:sameAs :OTRL4 ;
  .

:Otrl5
  owl:deprecated "true"^^xsd:boolean ;
  cc:deprecatedOn "2022-12-30"^^xsd:date ;
  schema:supersededBy :OTRL5 ;
  owl:sameAs :OTRL5 ;
  .

